;; Procedure to Set Game Clock
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
.proc SetGameClock
    inc Frame
    lda Frame
    cmp #60
    bne :+
        inc Clock60
        lda #0
        sta Frame
    :
    rts
.endproc